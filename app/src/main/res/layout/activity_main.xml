                android:onClick="addRightOperator"
                android:text="sin"
                android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                android:textColor="@color/text_1"
                android:textSize="20sp" />

            <Button
                android:id="@+id/funValBtn10"
